<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TableView"><attr format="integer" name="tableView_columnCount"/><attr format="integer" name="tableView_headerElevation"/><attr format="color" name="tableView_headerColor"/></declare-styleable>
    <item name="table_data_view" type="id"/>
    <item name="table_header_view" type="id"/>
    <string name="app_name">TableView</string>
    <string name="default_cell">Data [%1$d:%2$d]</string>
    <string name="default_header">Header %1$d</string>
    <string name="sortable_indicator_content_description">Sortable Indicator</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
</resources>